{

	"manifest_version": 2,
	"name" : "Freeze_It",
	"description" : "A Chrome extension for remembering the user browsing session",
	"version" : "1.0",
	"permissions":[
	 "tabs" ,
	 "unlimitedStorage"
	],
	"content_security_policy":"script-src 'self'; object-src 'self'",
	"browser_action": {
		"default_icon": "/images/icon.png",
		"default_popup": "main.html"
	}

}